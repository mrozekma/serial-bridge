{
	"name": "serial-bridge",
	"version": "2.0.0",
	"private": true,
	"scripts": {
		"package": "yarn build && node package.js",
		"build": "yarn server:build && yarn client:build",
		"dev": "concurrently -c bgRed.black,bgGreen.black -n server,client yarn:server:dev yarn:client:dev",
		"server:build": "cross-env NODE_ENV=production webpack",
		"server:run": "node -r esm -r ts-node/register src/server/server.ts",
		"server:dev": "cross-env NODE_ENV=development webpack --watch",
		"client:build": "vue-cli-service build",
		"client:dev": "cross-env NODE_ENV=development VUE_APP_SERVER_PORT=11000 vue-cli-service serve",
		"postinstall": "node patch.js"
	},
	"nodemonConfig": {
		"watch": [
			"src/server"
		],
		"execMap": {
			"ts": "node"
		}
	},
	"dependencies": {
		"@feathersjs/express": "^4.3.7",
		"@feathersjs/feathers": "^4.3.7",
		"@feathersjs/rest-client": "^4.3.7",
		"@feathersjs/socketio": "^4.3.7",
		"@feathersjs/socketio-client": "^4.3.7",
		"@hapi/joi": "^15.0.1",
		"activedirectory": "^0.7.2",
		"async-mutex": "^0.1.4",
		"axios": "^0.21.1",
		"deep-rename-keys": "^0.2.1",
		"filesize.js": "^2.0.0",
		"hash-files": "^1.1.1",
		"is-ip": "^3.1.0",
		"jszip": "^3.2.2",
		"node-json-db": "^1.0.2",
		"ora": "^4.0.2",
		"serialport": "^7.1.5",
		"slugify": "^1.3.5",
		"socket.io-client": "^2.3.0",
		"typesafe-joi": "^2.0.6",
		"typescript": "^3.7.0-beta",
		"url-parse": "^1.4.7",
		"xml2js": "^0.4.22"
	},
	"devDependencies": {
		"@types/axios": "^0.14.0",
		"@types/cors": "^2.8.6",
		"@types/jquery": "^3.3.31",
		"@types/json5": "^0.0.30",
		"@types/jszip": "^3.1.6",
		"@types/node": "^12.7.9",
		"@types/prismjs": "^1.16.0",
		"@types/serialport": "^7.0.4",
		"@types/url-parse": "^1.4.3",
		"@types/xml2js": "^0.4.5",
		"@vue/cli-plugin-babel": "^3.11.0",
		"@vue/cli-plugin-typescript": "^3.11.0",
		"@vue/cli-service": "^3.11.0",
		"animate.css": "^3.7.2",
		"ant-design-vue": "^1.3.16",
		"babel-plugin-prismjs": "^1.1.1",
		"chalk": "^2.4.2",
		"concurrently": "^4.1.2",
		"core-js": "^2.6.5",
		"cors": "^2.8.5",
		"cross-env": "^6.0.3",
		"esm": "^3.2.25",
		"fs-extra": "^8.1.0",
		"golden-layout": "^1.5.9",
		"jquery": "^3.4.1",
		"less": "^3.0.4",
		"less-loader": "^5.0.0",
		"nodemon": "^1.19.3",
		"nodemon-webpack-plugin": "^4.1.1",
		"postinstall-postinstall": "^2.0.0",
		"prismjs": "^1.17.1",
		"raw-loader": "^3.1.0",
		"synp": "^1.7.0",
		"ts-loader": "^6.2.0",
		"ts-node": "^8.4.1",
		"vue": "^2.6.10",
		"vue-property-decorator": "^8.2.2",
		"vue-template-compiler": "^2.6.10",
		"webpack": "^4.41.0",
		"webpack-cli": "^3.3.9",
		"webpack-node-externals": "^1.7.2",
		"xterm": "^4.1.0",
		"xterm-addon-fit": "^0.2.1",
		"zip-dir": "^1.0.2"
	}
}
